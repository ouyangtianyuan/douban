/**app.wxss**/
page{
  background:#f7f7f7;
  padding-top:130rpx;
  color:#424242;
}
.container{
  height: 100%;
  width:100%;
  display:flex;
  flex-direction: column;
}

.search-page{
  display:flex;
  padding:0 30rpx;
  height:100rpx;
  align-items: center;
  border-width:1px 0;
  border:1px solid #eeee;
  background:#fff;
  margin-bottom:30rpx;
  position:fixed;
  top:0;
  left:0;
  width:100%;
  box-sizing:border-box;
  z-index: 100;
}
.search-icon{
  margin-right:24rpx;
}
.search-text{
  color:#8d8d8d;
}
